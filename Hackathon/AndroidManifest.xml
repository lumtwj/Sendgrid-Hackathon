<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.enigma.hackathon"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BlockActivity"
            android:label="@string/title_activity_block" >
        </activity>
        <activity
            android:name=".GraphActivity"
            android:label="@string/title_activity_graph" >
        </activity>
        <activity
            android:name=".BounceActivity"
            android:label="@string/title_activity_bounce" >
        </activity>
        <activity
            android:name=".InvalidEmailActivity"
            android:label="@string/title_activity_invalid_email" >
        </activity>
        <activity
            android:name=".UnsubscribesActivity"
            android:label="@string/title_activity_unsubscribes" >
        </activity>
        <activity
            android:name=".SpamReportActivity"
            android:label="@string/title_activity_spam_report" >
        </activity>
        <activity
            android:name=".SendEmailActivity"
            android:label="@string/title_activity_send_email" >
        </activity>
    </application>

</manifest>
